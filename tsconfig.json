{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@icons/*": ["src/components/icons/*"],
      "@assets/*": ["src/assets/*"],
      "@layouts/*":["src/layouts/*"],
      "@constants":["src/utils/constants.ts"],
      "@interfaces":["src/utils/interfaces.ts"]
    }
  }
}